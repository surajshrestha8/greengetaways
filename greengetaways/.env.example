# Supabase PostgreSQL Connection String
# Use the "Connection Pooling" string from Supabase dashboard (port 6543, not 5432)
DATABASE_URI=postgresql://postgres.[project-ref]:[password]@aws-0-[region].pooler.supabase.com:6543/postgres

# Payload CMS Secret - Generate strong random string (min 32 characters)
# Generate with: openssl rand -base64 32
PAYLOAD_SECRET=YOUR_STRONG_SECRET_HERE_MIN_32_CHARS

# Next.js Server URL - Update after first Vercel deployment
NEXT_PUBLIC_SERVER_URL=https://your-app.vercel.app

# Node Environment
NODE_ENV=production
